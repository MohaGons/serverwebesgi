[DEFAULT]
bantime  = 10m
findtime  = 10m
maxretry = 5

[nginx-http-auth]
enabled = true
filter = nginx-http-auth
logpath = /var/log/nginx/error.log
maxretry = 3

[nginx-botsearch]
enabled = true
filter = nginx-botsearch
logpath = /var/log/nginx/access.log
maxretry = 2

[adminer]
enabled = true
filter = adminer
logpath = /var/log/nginx/adminer_access.log
maxretry = 3